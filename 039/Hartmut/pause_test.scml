<?xml version="1.0" encoding="UTF-8"?>
<tns:scml xsi:schemaLocation="http://www.ptb.de/epics/SCML scml.xsd"
    xmlns:tns="http://www.ptb.de/epics/SCML" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <version>0.4.1</version>
    <repeatcount>0</repeatcount>
    <chain id="1">
        <comment>keithley_spannung</comment>
        <savefilename>/messung/qnim/daten/MP_NEU/DATEN/mpneu_11_nov_10.hd5</savefilename>
        <confirmsave>false</confirmsave>
        <autonumber>true</autonumber>
        <savescandescription>false</savescandescription>
        <saveplugin name="HDF5">
            <parameter name="location">/messung/eve/lib/linux-x86/libhdf5plugin.so</parameter>
            <parameter name="format">HDF5</parameter>
            <parameter name="suffix">hd5</parameter>
            <parameter name="extent">0</parameter>
        </saveplugin>
        <scanmodules>
            <scanmodule id="1">
                <type>classic</type>
                <name>pause_test</name>
                <xpos>105</xpos>
                <ypos>55</ypos>
                <parent>0</parent>
                <settletime>0.0</settletime>
                <triggerdelay>1.0</triggerdelay>
                <triggerconfirm>false</triggerconfirm>
                <saveaxispositions>before</saveaxispositions>
                <smaxis>
                    <axisid>PPSMC:gw23715000</axisid>
                    <stepfunction>Positionlist</stepfunction>
                    <positionmode>absolute</positionmode>
                    <positionlist>1.2
4.3
7.5
5</positionlist>
                </smaxis>
                <smchannel>
                    <channelid>ringCurrent1chan1</channelid>
                    <confirmtrigger>false</confirmtrigger>
                </smchannel>
            </scanmodule>
        </scanmodules>
    </chain>
    <plugins>
        <plugin type="save">
            <name>ASCII</name>
            <location></location>
            <parameter name="format" datatype="string" default="ASCII" mandatory="false"></parameter>
            <parameter name="suffix" datatype="string" default="txt" mandatory="false"></parameter>
            <parameter name="extent" datatype="int" default="0" mandatory="false"></parameter>
        </plugin>
        <plugin type="save">
            <name>HDF5</name>
            <location>/messung/eve/lib/linux-x86/libhdf5plugin.so</location>
            <parameter name="format" datatype="string" default="HDF5" mandatory="false"/>
            <parameter name="suffix" datatype="string" default="h5" mandatory="false"/>
            <parameter name="extent" datatype="int" default="0" mandatory="false"/>
        </plugin>
        <plugin type="position">
            <name>ReferenceAdd</name>
            <location>/path/to/referenceadd</location>
            <parameter name="summand" datatype="double" default="0.0" mandatory="true"></parameter>
            <parameter name="referenceaxis" datatype="axisid" default="" mandatory="true"></parameter>
        </plugin>
        <plugin type="position">
            <name>ReferenceMultiply</name>
            <location>/path/to/referenceadd</location>
            <parameter name="factor" datatype="double" default="0.0" mandatory="true"></parameter>
            <parameter name="referenceaxis" datatype="axisid" default="" mandatory="true"></parameter>
        </plugin>
        <plugin type="postscanpositioning">
            <name>GOTO-Peak</name>
            <location>/path/to/plugin3</location>
            <parameter name="direction" datatype="string"
                default="positive" mandatory="false">positive,negative</parameter>
        </plugin>
        <plugin type="postscanpositioning">
            <name>GOTO-Center</name>
            <location>/path/to/plugin3</location>
            <parameter name="direction" datatype="string"
                default="positive" mandatory="true">positive,negative</parameter>
        </plugin>
    </plugins>
    <detectors>
        <detector>
            <class>Ringstrom</class>
            <name>Ring_1</name>
            <id>ringCurrent1basic</id>
            <channel>
                <class></class>
                <name>Ring_1</name>
                <id>ringCurrent1chan1</id>
                <read>
                    <access method="GETCB" type="double" transport="ca" monitor="true">ringCurrent1.VAL</access>
                    <value type="double"></value>
                </read>
                <unit>
                    <access method="GETPUT" type="string" transport="ca">ringCurrent1.EGU</access>
                </unit>
            </channel>
        </detector>
    </detectors>
    <motors>
        <motor>
            <class></class>
            <name>PPSMC:gw237</name>
            <id>PPSMC:gw237</id>
            <axis>
                <class></class>
                <name>PP_Motor1</name>
                <id>PPSMC:gw23715000</id>
                <goto>
                    <access method="GETPUTCB" type="double"
                        transport="ca" monitor="true" timeout="100.0">PPSMC:gw23715000</access>
                    <value type="double"></value>
                </goto>
                <position>
                    <access method="GETCB" type="double" transport="ca">PPSMC:gw23715000.RBV</access>
                </position>
                <stop>
                    <access method="PUT" type="int" transport="ca">PPSMC:gw23715000.STOP</access>
                    <value type="int">1</value>
                </stop>
                <unit>
                    <access method="GETPUTCB" type="string" transport="ca">PPSMC:gw23715000.EGU</access>
                </unit>
                <deadband>
                    <access method="GETPUTCB" type="double" transport="ca">PPSMC:gw23715000.RDBD</access>
                </deadband>
                <offset>
                    <access method="GETPUTCB" type="double" transport="ca">PPSMC:gw23715000.OFF</access>
                </offset>
                <tweakvalue>
                    <access method="GETPUTCB" type="double" transport="ca">PPSMC:gw23715000.TWV</access>
                </tweakvalue>
                <tweakforward>
                    <access method="PUT" type="int" transport="ca">PPSMC:gw23715000.TWF</access>
                    <value type="int">1</value>
                </tweakforward>
                <tweakreverse>
                    <access method="PUT" type="int" transport="ca">PPSMC:gw23715000.TWR</access>
                    <value type="int">1</value>
                </tweakreverse>
            </axis>
        </motor>
    </motors>
    <devices/>
    <smselection>
        <stepfunction>Add,Multiply,File,Plugin,Positionlist</stepfunction>
        <smtype>classic, advanced</smtype>
    </smselection>
</tns:scml>
