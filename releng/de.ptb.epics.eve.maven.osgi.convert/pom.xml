        </artifact>
        <artifact>
        	<id>org.apache.commons:commons-lang3:3.3.2</id>
        </artifact>
        <artifact>
        	<id>org.apache.commons:commons-math3:3.1.1</id>
        </artifact>
        <artifact>
        	<id>org.apache.pdfbox:fontbox:1.8.2</id>
        </artifact>
        <artifact>
        	<id>org.apache.pdfbox:pdfbox:1.8.2</id>
        </artifact>
        <artifact>
        	<id>org.eclipse.ecf:org.apache.commons.logging:1.1.1.v201101211721</id>
        </artifact>
       </artifacts>
      </configuration>
     </execution>
    </executions>
   </plugin>
  </plugins>
 </build>

</project>
