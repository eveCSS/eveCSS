                    <value>
                        <access method="GETPUTCB" type="double" transport="ca">PPSMC:23715000.VELO</access>
                    </value>
                    <displaygroup>summary</displaygroup>
                </option>
                <option>
                    <name>Lock Local</name>
                    <id>PPSMC:23715dislocal</id>
                    <value>
                        <access method="GETPUTCB" type="string" transport="ca">PPSMC:23715dislocal</access>
                        <value type="string">Enable,Disable</value>
                    </value>
                </option>
            </axis>
            <axis>
                <name>Y-Position</name>
                <id>PPSMC:23715001</id>
