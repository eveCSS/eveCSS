            <name>TestMotor</name>
            <id>PPSMC:23715</id>
            <unit>
                <access method="GETCB" transport="ca" type="string">PPSMC:23715000.EGU</access>
            </unit>
            <axis>
                <name>X-Position</name>
                <id>PPSMC:23715000</id>
