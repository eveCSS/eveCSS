    <detectors>
        <detector>
            <class>ringCurrent</class>
            <name>BESSYRingStrom</name>
            <id>ringCurrent</id>
            <unit>
                <unitstring>Ampere</unitstring>
            </unit>
            <channel>
                <name>RingStrom</name>
                <id>ringCurrent1</id>
                <read>
                    <access method="GETCB" transport="ca" type="double">ringCurrent1</access>
                </read>
            </channel>
        </detector>
        <detector>
            <class>Keithley617</class>
            <name>Keithley 617</name>
            <id>K0617:22225</id>
            <unit>
                <access method="GETCB" transport="ca" type="string">K0617:22225.EGU</access>
            </unit>
            <trigger>
                <access method="PUTCB" transport="ca" type="int">K0617:22225.TRIG</access>
                <value type="int">1</value>
            </trigger>
            <channel>
                <name>Kanal 1</name>
                <id>K0617:22225.1</id>
                <read>
                    <access method="GETCB" transport="ca" type="double">K0617:22225.VAL</access>
                </read>
            </channel>
            <option>
                <name>ZeroCheck</name>
                <id>K6559:22225zeroChk</id>
                <value>
                    <access method="GETPUTCB" transport="ca" type="OnOff">K6559:22225zeroChk</access>
                    <value type="OnOff">1,0</value>
                </value>
                <displaygroup>detail</displaygroup>
            </option>
            <option>
                <name>Function</name>
                <id>K6559:22225function</id>
                <value>
                    <access method="GETPUTCB" transport="ca" type="string">K6559:22225function</access>
                    <value type="string">Voltage,Current,Resistance</value>
                </value>
                <displaygroup>summary</displaygroup>
            </option>
        </detector>
        <detector>
            <class>Prema5000</class>
            <name></name>
            <id>23707</id>
            <unit>
                <access method="GETCB" transport="ca" type="double">P5000:2370700.EGU</access>
            </unit>
            <trigger>
                <access method="PUTCB" transport="ca" type="double">P5000:2370700.TRIG</access>
                <value type="int">1</value>
            </trigger>
            <channel>
                <name>Kanal 1</name>
                <id>P5000:2370700.1</id>
                <read>
                    <access method="GETCB" transport="ca" type="double">P5000:2370700.VAL</access>
                </read>
            </channel>
            <option>
                <name>Function</name>
                <id>P5000:23707function</id>
                <value>
                    <access method="GETPUTCB" transport="ca" type="string">P5000:23707function</access>
                    <value type="string">DCVolts, ACVolts, DCAmps, ACAmps, Ohms</value>
                </value>
                <displaygroup>summary</displaygroup>
            </option>
            <option>
                <name>Range</name>
                <id>P5000:23707range</id>
                <value>
                    <access method="GETPUTCB" transport="ca" type="string">P5000:23707range</access>
                    <value type="string">"Auto On","0.2 -> V kO","2 -> V mA kO","20 -> V kO","200 -> V kO","2 -> A MO","12 -> MO","Auto Off"</value>
                </value>
                <displaygroup>summary</displaygroup>
            </option>
        </detector>
    </detectors>
