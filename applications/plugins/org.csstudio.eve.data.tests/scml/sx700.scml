<?xml version="1.0" encoding="UTF-8"?>
<tns:scml xsi:schemaLocation="http://www.ptb.de/epics/SCML scml.xsd"
    xmlns:tns="http://www.ptb.de/epics/SCML" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <version>0.4.1</version>
    <repeatcount>0</repeatcount>
    <chain id="1">
        <savefilename>test.h5</savefilename>
        <confirmsave>false</confirmsave>
        <autonumber>false</autonumber>
        <savescandescription>false</savescandescription>
        <saveplugin name="HDF5">
            <parameter name="location">/messung/eve/lib/linux-x86/libhdf5plugin.so</parameter>
            <parameter name="format">HDF5</parameter>
            <parameter name="suffix">h5</parameter>
            <parameter name="extent">0</parameter>
        </saveplugin>
        <scanmodules>
            <scanmodule id="1">
                <type>classic</type>
                <name>SM 1</name>
                <xpos>217</xpos>
                <ypos>73</ypos>
                <parent>0</parent>
                <settletime>0.0</settletime>
                <triggerdelay>0.0</triggerdelay>
                <triggerconfirm>false</triggerconfirm>
                <saveaxispositions>never</saveaxispositions>
                <smaxis>
                    <axisid>OMS58:21602001</axisid>
                    <stepfunction>Add</stepfunction>
                    <positionmode>absolute</positionmode>
                    <start type="double">0.0</start>
                    <stop type="double">0.0</stop>
                    <stepwidth type="double">0</stepwidth>
                    <ismainaxis>false</ismainaxis>
                </smaxis>
            </scanmodule>
        </scanmodules>
    </chain>
    <plugins>
        <plugin type="save">
            <name>ASCII</name>
            <location></location>
            <parameter name="format" datatype="string" default="ASCII" mandatory="false"></parameter>
            <parameter name="suffix" datatype="string" default="txt" mandatory="false"></parameter>
            <parameter name="extent" datatype="int" default="0" mandatory="false"></parameter>
        </plugin>
        <plugin type="save">
            <name>HDF5</name>
            <location>/messung/eve/lib/linux-x86/libhdf5plugin.so</location>
            <parameter name="format" datatype="string" default="HDF5" mandatory="false"/>
            <parameter name="suffix" datatype="string" default="h5" mandatory="false"/>
            <parameter name="extent" datatype="int" default="0" mandatory="false"/>
        </plugin>
        <plugin type="position">
            <name>ReferenceAdd</name>
            <location>/path/to/referenceadd</location>
            <parameter name="summand" datatype="double" default="0.0" mandatory="true"></parameter>
            <parameter name="referenceaxis" datatype="axisid" default="" mandatory="true"></parameter>
        </plugin>
        <plugin type="position">
            <name>ReferenceMultiply</name>
            <location>/path/to/referenceadd</location>
            <parameter name="factor" datatype="double" default="0.0" mandatory="true"></parameter>
            <parameter name="referenceaxis" datatype="axisid" default="" mandatory="true"></parameter>
        </plugin>
        <plugin type="postscanpositioning">
            <name>GOTO-Peak</name>
            <location>/path/to/plugin3</location>
            <parameter name="direction" datatype="string"
                default="positive" mandatory="false">positive,negative</parameter>
        </plugin>
        <plugin type="postscanpositioning">
            <name>GOTO-Center</name>
            <location>/path/to/plugin3</location>
            <parameter name="direction" datatype="string"
                default="positive" mandatory="true">positive,negative</parameter>
        </plugin>
    </plugins>
    <detectors/>
    <motors>
        <motor>
            <class>BigRef</class>
            <name>OMS58:21602</name>
            <id>OMS58:21602</id>
            <axis>
                <class></class>
                <name>2Theta</name>
                <id>OMS58:21602001</id>
                <goto>
                    <access method="GETPUTCB" type="double"
                        transport="ca" monitor="true" timeout="100.0">OMS58:21602001</access>
                    <value type="double"></value>
                </goto>
                <position>
                    <access method="GETCB" type="double" transport="ca">OMS58:21602001.RBV</access>
                </position>
                <stop>
                    <access method="PUT" type="int" transport="ca">OMS58:21602001.STOP</access>
                    <value type="int">1</value>
                </stop>
                <unit>
                    <access method="GETPUTCB" type="string" transport="ca">OMS58:21602001.EGU</access>
                </unit>
                <deadband>
                    <access method="GETPUTCB" type="double" transport="ca">OMS58:21602001.RDBD</access>
                </deadband>
                <offset>
                    <access method="GETPUTCB" type="double" transport="ca">OMS58:21602001.OFF</access>
                </offset>
                <tweakvalue>
                    <access method="GETPUTCB" type="double" transport="ca">OMS58:21602001.TWV</access>
                </tweakvalue>
                <tweakforward>
                    <access method="PUT" type="int" transport="ca">OMS58:21602001.TWF</access>
                    <value type="int">1</value>
                </tweakforward>
                <tweakreverse>
                    <access method="PUT" type="int" transport="ca">OMS58:21602001.TWR</access>
                    <value type="int">1</value>
                </tweakreverse>
            </axis>
        </motor>
    </motors>
    <devices/>
    <smselection>
        <stepfunction>Add,Multiply,File,Plugin,Positionlist</stepfunction>
        <smtype>classic, advanced</smtype>
    </smselection>
</tns:scml>
