				<fo:table-cell font-weight="bold" padding-left="2pt" padding-right="2pt" padding-top="2pt" padding-bottom="2pt"><fo:block>Class Name </fo:block></fo:table-cell>
				<fo:table-cell font-weight="bold" padding-left="2pt" padding-right="2pt" padding-top="2pt" padding-bottom="2pt"><fo:block>Location </fo:block></fo:table-cell>
			</fo:table-row><fo:table-row>
				<fo:table-cell padding-left="2pt" padding-right="2pt" padding-top="2pt" padding-bottom="2pt"><fo:block>Motor </fo:block></fo:table-cell>
				<fo:table-cell padding-left="2pt" padding-right="2pt" padding-top="2pt" padding-bottom="2pt"><fo:block>C </fo:block></fo:table-cell>
				<fo:table-cell padding-left="2pt" padding-right="2pt" padding-top="2pt" padding-bottom="2pt"><fo:block>child of root element „C” </fo:block></fo:table-cell>
			</fo:table-row><fo:table-row>
				<fo:table-cell padding-left="2pt" padding-right="2pt" padding-top="2pt" padding-bottom="2pt"><fo:block>— </fo:block></fo:table-cell>
				<fo:table-cell padding-left="2pt" padding-right="2pt" padding-top="2pt" padding-bottom="2pt"><fo:block>child of root element „Motors &amp; Axes” </fo:block></fo:table-cell>
			</fo:table-row><fo:table-row>
				<fo:table-cell padding-left="2pt" padding-right="2pt" padding-top="2pt" padding-bottom="2pt"><fo:block>Motor Axis </fo:block></fo:table-cell>
				<fo:table-cell padding-left="2pt" padding-right="2pt" padding-top="2pt" padding-bottom="2pt"><fo:block>C </fo:block></fo:table-cell>
				<fo:table-cell padding-left="2pt" padding-right="2pt" padding-top="2pt" padding-bottom="2pt"><fo:block>child of root element „C” </fo:block></fo:table-cell>
