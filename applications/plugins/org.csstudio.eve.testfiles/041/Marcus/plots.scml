            <id>Timer1-mot</id>
            <axis>
                <class></class>
                <name>Timer</name>
                <id>Timer1-mot-axis</id>
                <goto>
                    <access method="PUTCB" type="datetime" transport="local">Timer</access>
                </goto>
                <position>
                    <access method="GETCB" type="datetime" transport="local">Timer</access>
                </position>
                <stop>
                    <access method="PUT" type="int" transport="local">Timer</access>
                    <value type="OnOff">0</value>
                </stop>
            </axis>
        </motor>
        <motor>
            <class>Counter</class>
            <name>Counter1</name>
            <id>Counter1</id>
            <axis>
                <class></class>
                <name>Counter</name>
                <id>Counter-mot</id>
                <goto>
                    <access method="PUTCB" type="int" transport="local">Counter</access>
                </goto>
                <position>
                    <access method="GETCB" type="int" transport="local">Counter</access>
                </position>
                <stop>
                    <access method="PUT" type="int" transport="local">Counter</access>
                    <value type="OnOff">0</value>
                </stop>
            </axis>
        </motor>
        <motor>
            <class></class>
            <name>PPSMC:gw237</name>
            <id>PPSMC:gw237</id>
            <axis>
                <class></class>
                <name>PP_Motor1</name>
                <id>PPSMC:gw23715000</id>
                <goto>
                    <access method="GETPUTCB" type="double"
                        transport="ca" monitor="true" timeout="100.0">PPSMC:gw23715000</access>
                    <value type="double"></value>
                </goto>
                <position>
                    <access method="GETCB" type="double" transport="ca">PPSMC:gw23715000.RBV</access>
                </position>
                <stop>
                    <access method="PUT" type="int" transport="ca">PPSMC:gw23715000.STOP</access>
                    <value type="int">1</value>
                </stop>
                <unit>
                    <access method="GETPUTCB" type="string" transport="ca">PPSMC:gw23715000.EGU</access>
                </unit>
                <deadband>
                    <access method="GETPUTCB" type="double" transport="ca">PPSMC:gw23715000.RDBD</access>
                </deadband>
                <offset>
                    <access method="GETPUTCB" type="double" transport="ca">PPSMC:gw23715000.OFF</access>
                </offset>
                <tweakvalue>
                    <access method="GETPUTCB" type="double" transport="ca">PPSMC:gw23715000.TWV</access>
                </tweakvalue>
                <tweakforward>
                    <access method="PUT" type="int" transport="ca">PPSMC:gw23715000.TWF</access>
                    <value type="int">1</value>
                </tweakforward>
                <tweakreverse>
                    <access method="PUT" type="int" transport="ca">PPSMC:gw23715000.TWR</access>
                    <value type="int">1</value>
                </tweakreverse>
            </axis>
        </motor>
    </motors>
