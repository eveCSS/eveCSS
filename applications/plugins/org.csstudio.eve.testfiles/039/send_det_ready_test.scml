<?xml version="1.0" encoding="UTF-8"?>
<tns:scml xsi:schemaLocation="http://www.ptb.de/epics/SCML scml.xsd"
    xmlns:tns="http://www.ptb.de/epics/SCML" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <version>0.3.7</version>
    <chain id="1">
        <confirmsave>false</confirmsave>
        <autonumber>false</autonumber>
        <savescandescription>false</savescandescription>
        <scanmodules>
            <scanmodule id="1">
                <type>classic</type>
                <name>New Scan Modul</name>
                <xpos>175</xpos>
                <ypos>130</ypos>
                <parent>0</parent>
                <settletime>0.0</settletime>
                <triggerdelay>0.0</triggerdelay>
                <triggerconfirm>false</triggerconfirm>
                <saveaxispositions>never</saveaxispositions>
                <smaxis>
                    <axisid>PPSMC:gw23715000</axisid>
                    <stepfunction>Add</stepfunction>
                    <positionmode>absolute</positionmode>
                    <start type="double">10.0</start>
                    <stop type="double">10.0</stop>
                    <stepwidth type="double">0</stepwidth>
                    <ismainaxis>false</ismainaxis>
                </smaxis>
                <smchannel>
                    <channelid>ringCurrent1chan1</channelid>
                    <confirmtrigger>false</confirmtrigger>
                    <sendreadyevent>true</sendreadyevent>
                </smchannel>
                <smchannel>
                    <channelid>ringCurrent2chan1</channelid>
                    <confirmtrigger>false</confirmtrigger>
                    <sendreadyevent>true</sendreadyevent>
                </smchannel>
                <smchannel>
                    <channelid>P5000:gw2370700.1</channelid>
                    <confirmtrigger>false</confirmtrigger>
                </smchannel>
            </scanmodule>
        </scanmodules>
    </chain>
    <plugins>
        <plugin type="position">
            <name>plugin1</name>
            <location>/path/to/plugin1</location>
        </plugin>
        <plugin type="position">
            <name>ReferenceAdd</name>
            <location>/path/to/referenceadd</location>
            <parameter name="summand" datatype="double" default="0.0" mandatory="true"></parameter>
            <parameter name="referenceaxis" datatype="axisid" default="" mandatory="true"></parameter>
        </plugin>
        <plugin type="position">
            <name>ReferenceMultiply</name>
            <location>/path/to/referenceadd</location>
            <parameter name="factor" datatype="double" default="0.0" mandatory="true"></parameter>
            <parameter name="referenceaxis" datatype="axisid" default="" mandatory="true"></parameter>
        </plugin>
        <plugin type="position">
            <name>plugin2</name>
            <location>/path/to/plugin2</location>
        </plugin>
        <plugin type="save">
            <name>ASCII</name>
            <location></location>
            <parameter name="format" datatype="string" default="ASCII" mandatory="false"></parameter>
            <parameter name="suffix" datatype="string" default="txt" mandatory="false"></parameter>
        </plugin>
        <plugin type="save">
            <name>HDF5</name>
            <location>/messung/eve/lib/linux-x86/libhdf5plugin.so</location>
            <parameter name="format" datatype="string" default="HDF5" mandatory="false"></parameter>
            <parameter name="suffix" datatype="string" default="hd5" mandatory="false"></parameter>
        </plugin>
        <plugin type="save">
            <name>HDFspezial</name>
            <location>HDFspezial.slib</location>
            <parameter name="format" datatype="string" default="classic" mandatory="true">classic,tables</parameter>
            <parameter name="buffersize" datatype="int" default="0" mandatory="true"></parameter>
        </plugin>
        <plugin type="postscanpositioning">
            <name>GOTO-Peak</name>
            <location>/path/to/plugin3</location>
            <parameter name="direction" datatype="string"
                default="positive" mandatory="false">positive,negative</parameter>
        </plugin>
        <plugin type="postscanpositioning">
            <name>GOTO-Center</name>
            <location>/path/to/plugin3</location>
            <parameter name="direction" datatype="string"
                default="positive" mandatory="true">positive,negative</parameter>
        </plugin>
    </plugins>
    <detectors>
        <detector>
            <class>Ringstrom</class>
            <name>Ring_1_Det</name>
            <id>ringCurrent1basic</id>
            <channel>
                <class></class>
                <name>Ring_1</name>
                <id>ringCurrent1chan1</id>
                <read>
                    <access method="GETPUTCB" type="double" transport="ca">ringCurrent1.VAL</access>
                </read>
                <unit>
                    <access method="GETPUT" type="string" transport="ca">ringCurrent1.EGU</access>
                </unit>
            </channel>
        </detector>
        <detector>
            <class>Ringstrom</class>
            <name>Ring_2_Det</name>
            <id>ringCurrent2basic</id>
            <channel>
                <class></class>
                <name>Ring_2</name>
                <id>ringCurrent2chan1</id>
                <read>
                    <access method="GETPUTCB" type="double" transport="ca">ringCurrent2.VAL</access>
                </read>
                <unit>
                    <access method="GETPUT" type="string" transport="ca">ringCurrent2.EGU</access>
                </unit>
            </channel>
        </detector>
        <detector>
            <class>Prema5000</class>
            <name>Prema</name>
            <id>23707</id>
            <unit>
                <access method="GETCB" type="double" transport="ca">P5000:gw2370700.EGU</access>
            </unit>
            <trigger>
                <access method="PUTCB" type="double" transport="ca">P5000:gw2370700.TRIG</access>
                <value type="int">1</value>
            </trigger>
            <channel>
                <class></class>
                <name></name>
                <id>P5000:gw2370700.1</id>
                <read>
                    <access method="GETCB" type="double" transport="ca">P5000:gw2370700.VAL</access>
                </read>
            </channel>
            <option>
                <name>Function</name>
                <id>P5000:gw23707function</id>
                <value>
                    <access method="GETPUTCB" type="string" transport="ca">P5000:gw23707function</access>
                    <value type="string">DCVolts, ACVolts, DCAmps, ACAmps, Ohms</value>
                </value>
                <displaygroup>summary</displaygroup>
            </option>
            <option>
                <name>Range</name>
                <id>P5000:gw23707range</id>
                <value>
                    <access method="GETPUTCB" type="string" transport="ca">P5000:gw23707range</access>
                    <value type="string">Auto On, 0.2 -&gt;V kO, 2 -&gt;V mA kO, 20 -&gt;V kO, 200 -&gt;V kO, 2 -&gt;A MO, 12 -&gt;MO, Auto Off</value>
                </value>
                <displaygroup>summary</displaygroup>
            </option>
        </detector>
    </detectors>
    <motors>
        <motor>
            <class>Timer</class>
            <name>T1</name>
            <id>Timer01</id>
            <axis>
                <class></class>
                <name>Wecker</name>
                <id>Timer01.01</id>
                <goto>
                    <access method="PUTCB" type="datetime" transport="local">Timer</access>
                </goto>
                <position>
                    <access method="GETCB" type="datetime" transport="local">Timer</access>
                </position>
                <stop>
                    <access method="PUT" type="int" transport="local">Timer</access>
                    <value type="OnOff">0</value>
                </stop>
            </axis>
        </motor>
        <motor>
            <class>Timer</class>
            <name></name>
            <id>Timer02</id>
            <axis>
                <class></class>
                <name>Wecker 2</name>
                <id>Timer02.01</id>
                <goto>
                    <access method="PUTCB" type="datetime" transport="local">Timer</access>
                </goto>
                <position>
                    <access method="GETCB" type="datetime" transport="local">Timer</access>
                </position>
                <stop>
                    <access method="PUT" type="int" transport="local">Timer</access>
                    <value type="OnOff">0</value>
                </stop>
            </axis>
        </motor>
        <motor>
            <class>KlasseC1</class>
            <name>C1</name>
            <id>Counter01</id>
            <axis>
                <class></class>
                <name></name>
                <id>Counter01.01</id>
                <goto>
                    <access method="PUTCB" type="int" transport="local">Counter</access>
                </goto>
                <position>
                    <access method="GETCB" type="int" transport="local">Counter</access>
                </position>
                <stop>
                    <access method="PUT" type="int" transport="local">Counter</access>
                    <value type="OnOff">0</value>
                </stop>
            </axis>
        </motor>
        <motor>
            <class></class>
            <name></name>
            <id>Counter02</id>
            <axis>
                <class></class>
                <name></name>
                <id>Counter02.01</id>
                <goto>
                    <access method="PUTCB" type="int" transport="local">Counter</access>
                </goto>
                <position>
                    <access method="GETCB" type="int" transport="local">Counter</access>
                </position>
                <stop>
                    <access method="PUT" type="int" transport="local">Counter</access>
                    <value type="OnOff">4, 7</value>
                </stop>
            </axis>
        </motor>
        <motor>
            <class>PPSMC</class>
            <name>Kreuztisch</name>
            <id>PPSMC:gw23715</id>
            <unit>
                <access method="GETCB" type="string" transport="ca">PPSMC:gw23715000.EGU</access>
            </unit>
            <axis>
                <class></class>
                <name>X_Position</name>
                <id>PPSMC:gw23715000</id>
                <goto>
                    <access method="PUTCB" type="double" transport="ca">PPSMC:gw23715000</access>
                    <value type="double">10.0, 12.0, 13.1, 14.7, 16.8, 20.5, 23.7, 25.1, 28.9</value>
                </goto>
                <position>
                    <access method="GETCB" type="double" transport="ca">PPSMC:gw23715000.RBV</access>
                </position>
                <stop>
                    <access method="PUT" type="int" transport="ca">PPSMC:gw23715000.STOP</access>
                    <value type="int">1</value>
                </stop>
                <trigger>
                    <access method="PUTCB" type="int" transport="ca">PPSMC:gw23715000.TRIG</access>
                    <value type="int">1</value>
                </trigger>
                <option>
                    <name>Velocity</name>
                    <id>PPSMC:gw23715000.VELO</id>
                    <value>
                        <access method="GETPUTCB" type="double"
                            count="1" transport="ca">PPSMC:gw23715000.VELO</access>
                    </value>
                    <displaygroup>summary</displaygroup>
                </option>
            </axis>
            <axis>
                <class></class>
                <name>Y-Position</name>
                <id>PPSMC:gw23715001</id>
                <goto>
                    <access method="PUTCB" type="double" transport="ca">PPSMC:gw23715001</access>
                </goto>
                <position>
                    <access method="GETCB" type="double" transport="ca">PPSMC:gw23715001.RBV</access>
                </position>
                <stop>
                    <access method="PUT" type="int" transport="ca">PPSMC:gw23715001.STOP</access>
                    <value type="int">1</value>
                </stop>
                <trigger>
                    <access method="PUTCB" type="int" transport="ca">PPSMC:gw23715001.TRIG</access>
                    <value type="int">1</value>
                </trigger>
                <option>
                    <name>Velocity</name>
                    <id>PPSMC:gw23715001.VELO</id>
                    <value>
                        <access method="GETPUTCB" type="double"
                            count="1" transport="ca">PPSMC:gw23715001.VELO</access>
                    </value>
                    <displaygroup>summary</displaygroup>
                </option>
            </axis>
            <option>
                <name>Lock Local</name>
                <id>PPSMC:gw23715dislocal</id>
                <value>
                    <access method="GETPUTCB" type="string" transport="ca">PPSMC:gw23715dislocal</access>
                    <value type="string">Enable, Disable</value>
                </value>
            </option>
        </motor>
    </motors>
    <devices>
        <device>
            <class></class>
            <name>"Dummy-Topup[]"</name>
            <id>TOPUP:228</id>
            <value>
                <access method="GETCB" type="double" transport="ca" monitor="true">PPSMC:gw23715001.RBV</access>
                <value type="double"></value>
            </value>
        </device>
        <device>
            <class></class>
            <name>Dummy-Nr.2</name>
            <id>TOPUP:234</id>
            <value>
                <access method="GETCB" type="double" transport="ca" monitor="true">PPSMC:gw23715001.RBV</access>
                <value type="double"></value>
            </value>
        </device>
    </devices>
    <smselection>
        <stepfunction>Add,Multiply,File,Plugin,Positionlist</stepfunction>
        <linestyle>solid,dashed,dotted</linestyle>
        <color>blue,green,yellow,red,black</color>
        <markstyle>star,point,circle</markstyle>
        <smtype>classic, advanced</smtype>
    </smselection>
</tns:scml>
