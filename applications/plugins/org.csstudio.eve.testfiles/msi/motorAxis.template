                </tweakvalue>
                <tweakforward>
                    <access method="PUT" transport="ca" type="int">$(PV).TWF</access>
                    <value type="int">1</value>
                </tweakforward>
                <tweakreverse>
                    <access method="PUT" transport="ca" type="int">$(PV).TWR</access>
                    <value type="int">1</value>
                </tweakreverse>
                <option>
                    <name>Position Readback</name>
