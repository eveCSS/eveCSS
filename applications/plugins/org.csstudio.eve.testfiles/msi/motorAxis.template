            <axis>
                <name>$(Name)</name>
                <id>$(PV)</id>
                <goto>
                    <access method="GETPUTCB" transport="ca" type="double" timeout="100.0">$(PV)</access>
                </goto>
                <position>
                    <access method="GETCB" transport="ca" type="double">$(PV).RBV</access>
                </position>
                <stop>
                    <access method="PUT" transport="ca" type="int">$(PV).STOP</access>
                    <value type="int">1</value>
                </stop>
                <status>
                    <access method="GETCB" transport="ca" type="int">$(PV).MSTA</access>
                </status>
                <unit>
                    <access method="GETPUTCB" transport="ca" type="string">$(PV).EGU</access>
